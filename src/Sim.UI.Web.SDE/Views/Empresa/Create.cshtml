@model Sim.UI.Web.SDE.ViewModels.VMEmpresa

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>VMEmpresa</h4>
<hr />

<div class="row">
    <form asp-action="Create">

        <div class="form-floating mb-3" hidden>
            <label asp-for="Empresa_Id" class="control-label"></label>
            <input asp-for="Empresa_Id" class="form-control" />
            <span asp-validation-for="Empresa_Id" class="text-danger"></span>
        </div>

        <div class="row g-2 mb-2">
            <div class="col-md-3">
                <label asp-for="CNPJ" class="form-label" for="inputCNPJ"></label>
                <input asp-for="CNPJ" class="form-control" id="inputCNPJ" />
                <span asp-validation-for="CNPJ" class="text-danger"></span>
            </div>
            <div class="col-md-2">
                <label asp-for="Tipo" class="form-label" for="inputTipo"></label>
                <input asp-for="Tipo" class="form-control" id="inputTipo" />
                <span asp-validation-for="Tipo" class="text-danger"></span>
            </div>
            <div class="col-md-3">
                <label asp-for="Data_Abertura" class="form-label" for="inputAbertura"></label>
                <input asp-for="Data_Abertura" class="form-control" id="inputAbertura" />
                <span asp-validation-for="Data_Abertura" class="text-danger"></span>
            </div>
        </div>

        <div class="row g-2 mb-2">
            <div class="col-md">
                <label asp-for="Nome_Empresarial" class="form-label" for="inputNome"></label>
                <input asp-for="Nome_Empresarial" class="form-control" id="inputNome" />
                <span asp-validation-for="Nome_Empresarial" class="text-danger"></span>
            </div>
        </div>
        <div class="row g-2 mb-2">
            <div class="col-md">
                <label asp-for="Nome_Fantasia" class="form-label" for="inputFantasia"></label>
                <input asp-for="Nome_Fantasia" class="form-control" id="inputFantasia" />
                <span asp-validation-for="Nome_Fantasia" class="text-danger"></span>
            </div>
            <div class="col-md-2">
                <label asp-for="Porte" class="form-label" for="inputPorte"></label>
                <input asp-for="Porte" class="form-control" id="inputPorte" />
                <span asp-validation-for="Porte" class="text-danger"></span>
            </div>
        </div>

        <div class="row g-2 mb-2">
            <div class="col-md">
                <label asp-for="Atividade_Principal" class="form-label" for="inputAtividade"></label>
                <input asp-for="Atividade_Principal" class="form-control" id="inputAtividade" />
                <span asp-validation-for="Atividade_Principal" class="text-danger"></span>
            </div>
            <div class="col-md-2">
                <label asp-for="CNAE_Principal" class="form-label" for="inputCNAE"></label>
                <input asp-for="CNAE_Principal" class="form-control" id="inputCNAE" />
                <span asp-validation-for="CNAE_Principal" class="text-danger"></span>
            </div>
        </div>
        <div class="row g-2 mb-2">
            <div class="col-md">
                <label asp-for="Atividade_Secundarias" class="form-label" for="inputSecundaria"></label>
                <textarea asp-for="Atividade_Secundarias" class="form-control" id="inputSecundaria"></textarea>
                <span asp-validation-for="Atividade_Secundarias" class="text-danger"></span>
            </div>
        </div>
        <div class="row g-2 mb-2">
            <div class="col-md">
                <label asp-for="Natureza_Juridica" class="form-label" for="inputNatureza"></label>
                <input asp-for="Natureza_Juridica" class="form-control" id="inputNatureza" />
                <span asp-validation-for="Natureza_Juridica" class="text-danger"></span>
            </div>
        </div>
        <div class="row g-2 mb-2">
            <div class="col-md-2">
                <label asp-for="CEP" class="form-label" for="inputCEP"></label>
                <input asp-for="CEP" class="form-control" id="inputCEP" />
                <span asp-validation-for="CEP" class="text-danger"></span>
            </div>
            <div class="col-md">
                <label asp-for="Logradouro" class="form-label" for="inputRua"></label>
                <input asp-for="Logradouro" class="form-control" id="inputRua" />
                <span asp-validation-for="Logradouro" class="text-danger"></span>
            </div>
            <div class="col-md-2">
                <label asp-for="Numero" class="form-label" for="inputNumero"></label>
                <input asp-for="Numero" class="form-control" id="inputNumero" />
                <span asp-validation-for="Numero" class="text-danger"></span>
            </div>
        </div>
        <div class="row g-2 mb-2">
            <div class="col-md-2">
                <label asp-for="Complemento" class="form-label" for="inputComp"></label>
                <input asp-for="Complemento" class="form-control" id="inputComp" />
                <span asp-validation-for="Complemento" class="text-danger"></span>
            </div>
            <div class="col-md">
                <label asp-for="Bairro" class="form-label" for="inputBairro"></label>
                <input asp-for="Bairro" class="form-control" id="inputBairro" />
                <span asp-validation-for="Bairro" class="text-danger"></span>
            </div>
            <div class="col-md">
                <label asp-for="Municipio" class="form-label" for="inputMunucipio"></label>
                <input asp-for="Municipio" class="form-control" id="inputMunucipio" />
                <span asp-validation-for="Municipio" class="text-danger"></span>
            </div>
            <div class="col-md-2">
                <label asp-for="UF" class="form-label" for="inputUF"></label>
                <input asp-for="UF" class="form-control" id="inputUF" />
                <span asp-validation-for="UF" class="text-danger"></span>
            </div>
        </div>
        <div class="row g-2 mb-2">
            <div class="col-md">
                <label asp-for="Email" class="form-label" for="inputEmail"></label>
                <input asp-for="Email" class="form-control" id="inputEmail" />
                <span asp-validation-for="Email" class="text-danger"></span>
            </div>
            <div class="col-md-4">
                <label asp-for="Telefone" class="form-label" for="inputTel"></label>
                <input asp-for="Telefone" class="form-control" id="inputTel" />
                <span asp-validation-for="Telefone" class="text-danger"></span>
            </div>
        </div>
        <div class="row g-2 mb-2">
            <div class="col-md">
                <label asp-for="Ente_Federativo_Resp" class="form-label" for="inputEntF"></label>
                <input asp-for="Ente_Federativo_Resp" class="form-control" id="inputEntF" />
                <span asp-validation-for="Ente_Federativo_Resp" class="text-danger"></span>
            </div>
        </div>
        <div class="row g-2 mb-2">
            <div class="col-md">
                <label asp-for="Situacao_Cadastral" class="form-label" for="inputSitCad"></label>
                <input asp-for="Situacao_Cadastral" class="form-control" id="inputSitCad" />
                <span asp-validation-for="Situacao_Cadastral" class="text-danger"></span>
            </div>
            <div class="col-md-4">
                <label asp-for="Data_Situacao_Cadastral" class="form-label" for="inputSit"></label>
                <input asp-for="Data_Situacao_Cadastral" class="form-control" id="inputSit" />
                <span asp-validation-for="Data_Situacao_Cadastral" class="text-danger"></span>
            </div>
        </div>
        <div class="row g-2 mb-2">
            <div class="col-md">
                <label asp-for="Motivo_Situacao_Cadastral" class="form-label" for="inputMotivositcad"></label>
                <input asp-for="Motivo_Situacao_Cadastral" class="form-control" id="inputMotivositcad" />
                <span asp-validation-for="Motivo_Situacao_Cadastral" class="text-danger"></span>
            </div>
        </div>
        <div class="row g-2 mb-2">
            <div class="col-md">
                <label asp-for="Situacao_Especial" class="form-label" for="inputSitesp"></label>
                <input asp-for="Situacao_Especial" class="form-control" id="inputSitesp" />
                <span asp-validation-for="Situacao_Especial" class="text-danger"></span>
            </div>
            <div class="col-md-4">
                <label asp-for="Data_Situacao_Especial" class="form-label" for="datasitespe"></label>
                <input asp-for="Data_Situacao_Especial" class="form-control" id="datasitespe" />
                <span asp-validation-for="Data_Situacao_Especial" class="text-danger"></span>
            </div>
        </div>
        <div class="row g-2 mb-4">
            <div class="col-md">
                <label asp-for="Capital_Social" class="form-label" for="inputCapsoc"></label>
                <input asp-for="Capital_Social" class="form-control" id="inputCapsoc" />
                <span asp-validation-for="Capital_Social" class="text-danger"></span>
            </div>
        </div>
        <div class="row g-2 mb-2">
            <div class="d-grid gap-2 d-md-block">
                <a asp-action="Index" role="button" type="button" class="btn btn-secondary">Voltar</a>
                <button type="submit" role="button" class="btn btn-primary">Gravar</button>
            </div>
        </div>
    </form>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
    <script src="~/js/jquery.cep.js"></script>
    <script src="~/js/jquery.mask.min.js"></script>
    <script src="~/js/jquery.validation.js"></script>
    <script>
        $('#inputCNPJ').mask('00.000.000/0000-00', { placeholder: "__.___.___/____-__" });
        $('#inputTel').mask('(00) 0000-0000', { placeholder: "(__) ____-____" });
        $('#inputCEP').mask('00000-000', { placeholder: "_____-___" });
    </script>
}
